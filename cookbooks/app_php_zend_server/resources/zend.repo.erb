[Zend]
name=Zend Server
baseurl=<%= node[:zend][:server][:repo_baseurl]%><%= node[:zend][:server][:repo_suffix]%>/rpm/$basearch
enabled=1
gpgcheck=1
gpgkey=http://repos.zend.com/zend.key

[Zend_noarch]
name=Zend Server - noarch
baseurl=<%= node[:zend][:server][:repo_baseurl]%><%= node[:zend][:server][:repo_suffix]%>/rpm/noarch
enabled=1
gpgcheck=1
gpgkey=http://repos.zend.com/zend.key

